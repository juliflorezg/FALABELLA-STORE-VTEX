{
  "flex-layout.row#product-page__tabs": {
    "title": "row cont. for extra info, product page",
    "children": ["tab-layout#product-page__tabs"]
  },
  "tab-layout#product-page__tabs": {
    "title": "tab layout for product-page",
    "children": [
      "tab-list#product-page__tabs--list",
      "tab-content#product-page__tabs--content"
    ]
  },
  "tab-list#product-page__tabs--list": {
    "title": "tab list for product-page",
    "children": [
      "tab-list.item#tabs__list--product-specifications",
      "tab-list.item#tabs__list--reviews-and-ratings"
    ]
  },
  "tab-list.item#tabs__list--product-specifications": {
    "title": "tab list item, product specifications",
    "props": {
      "tabId": "product-specifications",
      "label": "Información adicional",
      "defaultActiveTab": true
    }
  },
  "tab-list.item#tabs__list--reviews-and-ratings": {
    "title": "tab list item, reviews & ratings",
    "props": {
      "tabId": "reviews-and-ratings",
      "label": "Revisiones y calificaciones"
    }
  },
  "tab-content#product-page__tabs--content": {
    "title": "tab content, product page",
    "children": [
      "tab-content.item#tabs__list--product-specifications",
      "tab-content.item#tabs__list--reviews-and-ratings"
    ]
  },

  "tab-content.item#tabs__list--product-specifications": {
    "title": "tab content item, product specifications",
    "children": ["flex-layout.row#product-specifications__content"],
    "props": {
      "tabId": "product-specifications"
    }
  },
  "flex-layout.row#product-specifications__content": {
    "title": "row cont. for product specifications section",
    "children": [
      "flex-layout.col#product-specifications__content--one",
      "flex-layout.col#product-specifications__content--two"
    ],
    "props": {
      "blockClass": "product-specifications__content"
    }
  },
  "flex-layout.col#product-specifications__content--one": {
    "title": "col cont. for product specifications",
    "children": ["product-specifications"]
  },
  "flex-layout.col#product-specifications__content--two": {
    "title": "col cont. for products additional info",
    "children": [
      "rich-text#content-two__title",
      "image#content-two__image-one",
      "video#content-two__video-one",
      // "sandbox#content-two__video-one",
      "disclosure-layout#content-two__disclosure"
    ]
    // "props": {
    //   "horizontalAlign": "center"
    // }
  },
  "rich-text#content-two__title": {
    "title": "first title, content two column, product page",
    "props": {
      "text": "## Información del Producto",
      "blockClass": "content-two__title"
    }
  },
  "image#content-two__image-one": {
    "title": "first image, content two column, product page",
    "props": {
      "src": "assets/img/global/product/additional_info-image1.png",
      "blockClass": "content-two__image-one"
    }
  },
  "video#content-two__video-one": {
    "title": "first video, content two column, product page",
    "props": {
      "src": "https://www.youtube.com/watch?v=0Pnj3J9q8ZI",
      "width": "80%",
      "controlsType": "native",
      "blockClass": "content-two__video-one"
    }
  },
