{
  "slider-layout#home__slider-tendencias--desktop": {
    "title": "slider de tendencias en desktop",
    "props": {
      "label": "slider principal",
      "itemsPerPage": {
        "desktop": 3
      },
      "blockClass": "home__slider-tendencias",
